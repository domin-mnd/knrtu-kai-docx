{
  "name": "desktop",
  "description": "Интерфейс конвертёра",
  "homepage": "https://github.com/domin-mnd/knrtu-kai-docx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domin-mnd/knrtu-kai-docx.git"
  },
  "version": "0.0.3",
  "main": "./out/main/index.js",
  "author": "domin-mnd",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "electron-builder --dir",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac --publish=always",
    "build:linux": "electron-builder --linux --publish=always"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@fontsource-variable/inter": "^5.0.17",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^18.19.9",
    "converter": "workspace:*",
    "electron": "^29.1.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.0.0",
    "sass": "^1.71.1",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
